{
  "name": "vid-ui-electron",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.bundle.js",
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --color",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --color",
    "jest": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 jest",
    "jest-ci": "npm run jest -- --config jest.config.ci.mjs",
    "watch-jest": "npm run jest -- --watchAll",
    "lint": "eslint . --cache --ext js,jsx,cjs,mjs,ts,tsx",
    "test": "npm run lint && npm run jest",
    "next-rc": "npm --no-git-tag-version version prerelease --preid=rc",
    "next-patch": "npm --no-git-tag-version version patch",
    "next-minor": "npm --no-git-tag-version version minor",
    "next-major": "npm --no-git-tag-version version major",
    "build:win": "electron-builder build --win",
    "build:mac": "electron-builder build --mac"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sth144/vid-ui-electron.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sth144/vid-ui-electron/issues"
  },
  "homepage": "https://github.com/sth144/vid-ui-electron#readme",
  "dependencies": {
    "@babel/runtime": "^7.22.0",
    "@electron/remote": "^2.0.9",
    "@techstark/opencv-js": "^4.7.0-release.1",
    "@types/electron": "^1.6.10",
    "electron": "^24.4.0",
    "fluent-ffmpeg": "^2.1.2",
    "inversify": "^6.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-video-renderer": "^2.5.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@material-ui/core": "^4.12.4",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/jest": "^29.1.2",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.4",
    "electron": "^21.1.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.1",
    "jest": "^29.1.2",
    "mini-css-extract-plugin": "^2.7.6",
    "rimraf": "^3.0.2",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.3",
